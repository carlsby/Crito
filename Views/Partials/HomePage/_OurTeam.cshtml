@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@using Umbraco.Cms.Core.Models.Blocks;
@using Umbraco.Cms.Core.Models
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.HomePage>

<section class="team-section">
    <div class="container team-con">
        <div class="row">
            <p class="team-title">Meet Our Team</p>
            <div class="col-8">
                <h1 class="team-header-title">Experience Team Members</h1>
            </div>
            <div class="col-4 d-flex justify-content-end">
                <button class="browse-team-btn">Browse Team <i class="fa-light fa-arrow-up-right"></i></button>
            </div>
        </div>
        <div class="row">
            @Html.GetBlockListHtml(Model.MemberBlockList)
        </div>

    </div>
</section>