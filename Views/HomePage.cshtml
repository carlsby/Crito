@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.HomePage>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "_Layout.cshtml";
    ViewData["PageTitle"] = @Model.Value("pageTitle");
    ViewData["PageDescription"] = @Model.Value("pageDescription");
    ViewData["PageKeywords"] = @Model.Value("pageKeywords");
    ViewData["PageCreator"] = @Model.Value("pageCreator");
}

@await Html.PartialAsync("HomePage/_Hero")
@await Html.PartialAsync("HomePage/_Logos")
@await Html.PartialAsync("HomePage/_Features")
@await Html.PartialAsync("HomePage/_AboutCompany")
@await Html.PartialAsync("HomePage/_Services")
@await Html.PartialAsync("HomePage/_WhyChooseUs")
@await Html.PartialAsync("HomePage/_Projects")
@await Html.PartialAsync("HomePage/_OurTeam")
@await Html.PartialAsync("HomePage/_Testimonial")
@await Html.PartialAsync("HomePage/_ArticleNews")
@await Html.PartialAsync("_Newsletter")