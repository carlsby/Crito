@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.HomePage>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "_Layout.cshtml";
    ViewData["PageTitle"] = @Model.Value("pageTitle");
    ViewData["PageDescription"] = @Model.Value("pageDescription");
    ViewData["PageKeywords"] = @Model.Value("pageKeywords");
    ViewData["PageCreator"] = @Model.Value("pageCreator");
}

@await Html.PartialAsync("_Hero")
@await Html.PartialAsync("_Logos")
@await Html.PartialAsync("_Features")
@await Html.PartialAsync("_AboutCompany")
@await Html.PartialAsync("_Services")
@await Html.PartialAsync("_WhyChooseUs")
@await Html.PartialAsync("_Projects")
@await Html.PartialAsync("_OurTeam")
@await Html.PartialAsync("_Testimonial")
@await Html.PartialAsync("_ArticleNews")
@await Html.PartialAsync("_Newsletter")