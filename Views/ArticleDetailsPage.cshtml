@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.ArticleDetailsPage>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "_Layout.cshtml";
    ViewData["PageTitle"] = @Model.Value("pageName");
    ViewData["PageDescription"] = @Model.Value("pageDescription");
    ViewData["PageKeywords"] = @Model.Value("pageKeywords");
    ViewData["PageCreator"] = @Model.Value("pageCreator");
}

@await Html.PartialAsync("_Breadcrumb")
@await Html.PartialAsync("ArticleDetailsPage/_Article")
@await Html.PartialAsync("HomePage/_ArticleNews")